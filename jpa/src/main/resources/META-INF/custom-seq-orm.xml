<entity-mappings
  xmlns="http://java.sun.com/xml/ns/persistence/orm"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
  version="1.0">
  
  <sequence-generator name="tutorial_sequence" sequence-name="jpa_tutorial_sequence"/>
    
  <entity class="net.familyyu.jeff.professor.model.Address" >
    <attributes>
      <id name="_id" >
        <column name="ID"/>
        <generated-value generator="tutorial_sequence" strategy="AUTO" />
      </id>
    </attributes>
  </entity>
  
  <entity class="net.familyyu.jeff.professor.model.Professor" >
    <attributes>
      <id name="_id" >
        <column name="ID"/>
        <generated-value generator="tutorial_sequence" strategy="AUTO"/>
      </id>
    </attributes>
  </entity>
  
  <entity class="net.familyyu.jeff.professor.model.State" >
    <attributes>
      <id name="_id" >
        <column name="ID"/>
        <generated-value generator="tutorial_sequence" strategy="AUTO"/>
      </id>
    </attributes>
  </entity>
  
  <entity class="net.familyyu.jeff.cms.model.Vendor" >
    <attributes>
      <id name="_id" >
        <column name="ID"/>
        <generated-value generator="tutorial_sequence" strategy="AUTO"/>
      </id>
    </attributes>
  </entity>
  
  <entity class="net.familyyu.jeff.cms.model.Location" >
    <attributes>
      <id name="_id" >
        <column name="ID"/>
        <generated-value generator="tutorial_sequence" strategy="AUTO"/>
      </id>
    </attributes>
  </entity>    
  
</entity-mappings>